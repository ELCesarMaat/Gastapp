{
  "ConnectionStrings": {
    "DefaultConnection": "Server={SERVER};Initial Catalog={DBName};Persist Security Info=False;User ID={USER};Password={PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "KEY" : "{SECRET_PASSWORD}"
  }

}
